# KaitoTwitterAPI Configuration
KAITO_API_TOKEN=your-kaito-api-token-here

# System Configuration
LOG_LEVEL=info
NODE_ENV=development
DEBUG=false
DATA_DIR=./data
TIMEZONE=Asia/Tokyo

# X (Twitter) Login Credentials for Playwright
X_USERNAME=your-x-username
X_PASSWORD=your-x-password
X_EMAIL=your-x-email
X_TOTP_SECRET=your-x-totp-secret

# Claude SDK のモック使用フラグ
USE_CLAUDE_MOCK=false

# Claude SDK開発モード（認証なしでモック使用）
# Claude CLIで認証が完了していない場合は true に設定
CLAUDE_SDK_DEV_MODE=false